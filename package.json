{
  "name": "LEDController",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/Nixplay/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "coverage": "jest --coverage",
    "ios:beta": "npm install && fastlane ios beta",
    "android:beta": "npm install && fastlane android beta"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.2",
    "react-native-ble-manager": "^6.2.6"
  },
  "devDependencies": {
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.3.0",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
